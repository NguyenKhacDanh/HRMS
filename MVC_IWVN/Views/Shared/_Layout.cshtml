<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                @if (Session["isManagerment"] != null && Session["isManagerment"].ToString() == "Managerment")
                {
                    @RenderPage("_Navigation.cshtml")

                }
                else
                {
                    @RenderPage("_NavigationUser.cshtml")
                }
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        <input type="text" id="Role_HR_update" value="@MVC_IWVN.Common.setRole.Role_HR_update.ToString()" style="visibility:hidden" />
                    </div>
                    <nav class="nav navbar-nav">
                        <ul class=" navbar-right">
                            <li class="nav-item dropdown open" style="padding-left: 15px;">
                                <a href="javascript:;" class="user-profile dropdown-toggle" aria-haspopup="true" id="navbarDropdown" data-toggle="dropdown" aria-expanded="false">
                                    <img src="~/Uploads/Images/logo.png" alt="">
                                    @if (Session["UserName"] != null)
                                    {
                                        @Session["UserName"].ToString()
                                    }
                                </a>
                                <div class="dropdown-menu dropdown-usermenu pull-right" aria-labelledby="navbarDropdown">
                                    @*<a class="dropdown-item" href="javascript:;"> @Resources.Content.Profile</a>*@
                                    <a class="dropdown-item" href="javascript:;" data-toggle="modal" data-target="#modalChangePassword">@Resources.Content.ChangePassword</a>
                                    @Html.ActionLink(Resources.Content.Logout, "Logout", "Home", null, new { @class = "dropdown-item" })
                                </div>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
            </div>

            <div class="modal fade" id="modalChangePassword" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">@Resources.Content.ChangePassword</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <form>
                            <div class="modal-body">

                                <div class="form-group">
                                    <label for="oldPassword">@Resources.Content.PasswordOld</label>
                                    <input type="password" class="form-control" id="oldPassword" required placeholder="@Resources.Content.PasswordOld">
                                    <span toggle="#password-field" id="old-password-field" class="fa fa-fw fa-eye field-icon toggle-password"></span>
                                </div>
                                <div class="form-group">
                                    <label for="newPassword">@Resources.Content.PasswordNew</label>
                                    <input type="password" class="form-control" id="newPassword" required placeholder="@Resources.Content.PasswordNew">
                                    <span toggle="#password-field" id="new-password-field" class="fa fa-fw fa-eye field-icon toggle-password"></span>
                                </div>
                                <div class="form-group">
                                    <label for="confirmPassword">@Resources.Content.PasswordConfirm</label>
                                    <input type="password" class="form-control" id="confirmPassword" required placeholder="@Resources.Content.PasswordConfirm">
                                    <span toggle="#password-field" id="confirm-password-field" class="fa fa-fw fa-eye field-icon toggle-password"></span>
                                </div>
                                <div id="resultMessage" class="alert" role="alert" style="display: none;"></div>

                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-info" id="btnChangePass">@Resources.Content.Save</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    <span>@Resources.Content.Language : </span>
                    <a href="@Url.Action("ChangeLanguage", "Home", new { lang = "vn" })">
                        <img src="~/Uploads/Images/Icon/IconVietNam.png" alt="Tiếng Việt" width="30">
                    </a>
                    <a href="@Url.Action("ChangeLanguage", "Home", new { lang = "en" })">
                        <img src="~/Uploads/Images/Icon/IconEnglish.png" alt="English" width="30">
                    </a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

